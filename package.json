{
  "name": "taoshi-gamehub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "uglify-flappy-taoshi": "uglifyjs public/games/flappyTaoshi/scripts/game.js -c -o public/games/flappyTaoshi/scripts/game.js",
    "uglify-taoshi-jump": "uglifyjs public/games/taoshiJump/scripts/game.js -c -o public/games/taoshiJump/scripts/game.js",
    "uglify-meme-invaders": "uglifyjs public/games/memeInvaders/scripts/game.js -c -o public/games/memeInvaders/scripts/game.js",
    "uglify-getLocalStorage": "uglifyjs public/scripts/getLocalStorageItem.js -c -o public/scripts/getLocalStorageItem.js",
    "uglify-setLocalStorage": "uglifyjs public/scripts/setLocalStorageItem.js -c -o public/scripts/setLocalStorageItem.js",
    "uglify": "npm run uglify-flappy-taoshi && npm run uglify-taoshi-jump && npm run uglify-meme-invaders && npm run uglify-getLocalStorage && npm run uglify-setLocalStorage",
    "build": "npm run uglify && next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "lint-build": "next lint && next build",
    "format-code": "prettier --write \"./**/*.{tsx,json}\"",
    "precommit": "npm run lint-build",
    "validate": "npm run precommit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/devtools-detect": "^3.0.0",
    "@types/react-responsive": "^8.0.8",
    "@vercel/analytics": "^1.2.2",
    "@vercel/postgres": "^0.8.0",
    "devtools-detect": "^4.0.2",
    "file-loader": "^6.2.0",
    "framer-motion": "^11.0.25",
    "js-md5": "^0.8.3",
    "lodash": "^4.17.21",
    "next": "14.1.3",
    "next-connect": "^1.0.0",
    "next-seo": "^6.0.0",
    "next-sitemap": "^4.0.0",
    "react": "18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "18.2.0",
    "react-responsive": "^10.0.0",
    "react-select": "^5.8.0",
    "react-type-animation": "^3.2.0",
    "uglify-js": "^3.17.4",
    "use-sound": "^4.0.1",
    "vercel": "^33.7.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.4",
    "@types/node": "20.11.25",
    "@types/react": "18.2.65",
    "@types/react-dom": "18.2.22",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "husky": "^9.0.0",
    "postcss": "^8.4.21",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "typescript": "5.4.2"
  }
}
